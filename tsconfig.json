// basic gatsbyjs tsconfig.json
{
    "compilerOptions": {
      "module": "commonjs",
      "target": "esnext",
      "jsx": "preserve",
      "lib": ["dom", "esnext"],
      "strict": true,
      "noEmit": true,
      "isolatedModules": true,
      "noUnusedLocals": false,
      "esModuleInterop": true,
    },
    "exclude": ["node_modules", "public", ".cache"]
  }
  